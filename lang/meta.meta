meta meta {
  grammar   = 'meta' token '{' rule+ '}',
  
  rule      = token '=' expr ',',
  
  expr      = choice
            | seq
            | plus
            | star
            | exprLeaf,
            
  exprLeaf  = token
            | literal,
  
  plus      = exprLeaf '+',
  
  star      = exprLeaf '*',
  
  exprA     = plus
            | star
            | exprLeaf,
  
  seq       = exprA+,
  
  choiceA   = '|' exprA,
  
  choice    = exprA choiceA+,
}
